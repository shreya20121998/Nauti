<mvc:View controllerName="nauticalfe.controller.TransView"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core"
    xmlns="sap.m" xmlns:tnt="sap.tnt">
    <Page id="_IDGenPage1">
        <tnt:ToolHeader id="dashHeaderId"><Text id="dashHeading" text="Voyage Transaction Data" class="dashHeading1"></Text></tnt:ToolHeader>

        <VBox id="_IDGenVBox1"  class="vPanel">

            <Panel id="_IDGenPanel1"  class="panel" width="700px" height="auto">

                <ObjectListItem id="_IDGenObjectListItem1"  intro="Cockpit" ></ObjectListItem>

                <Button id="_IDGenButton2" text="Voyage"  width="30%" class="voy"></Button>
                <Button id="_IDGenButton4" icon="sap-icon://arrow-right" class="right"></Button>
                <Button id="_IDGenButton3"  text="Freight Simulator" width="30%" class="FS"></Button>
                <Button id="_IDGenButton5"  text="Chartering" width="30%" icon="sap-icon://bar-chart" class="voy"></Button>
                <Button id="_IDGenButton6" icon="sap-icon://arrow-right" class="right"></Button>
                <Button id="_IDGenButton7"  text="Request for Quotation" width="30%" class="FS"></Button>
                <Button id="_IDGenButton8"  text="Compare Request for Quotation" width="30%" class="voy"></Button>
                <Button id="_IDGenButton9" icon="sap-icon://arrow-right" class="right"></Button>
                <Button id="_IDGenButton10"  text="Invite for Live Freight Negotiation" width="30%" class="FS"></Button>
                <Button id="_IDGenButton11"  text="Compare Live Freight Negotiation" width="30%" class="voy"></Button>
                <Button id="_IDGenButton12" icon="sap-icon://arrow-right" class="right"></Button>
                <Button id="_IDGenButton13"  text="Reports" width="30%" class="FS"></Button>
            </Panel>   

            <Panel id="_IDGenPanel2"  class="panel1">
                <Image id="_IDGenImage1" class="mastrimg"   src="../images/nautical.png" width="100%" height="auto"></Image>

            <!-- <Image id="_IDGenImage1"  src="https://www.shutterstock.com/image-photo/container-ship-vessel-cargo-carrier-260nw-1457257346.jpg" width="600px" height="500px" class="img"></Image> -->
            </Panel>
        </VBox>
    </Page>
 
</mvc:View>